import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        int x, y;
        Scanner input = new Scanner(System.in);
        System.out.println("Üssü alınacak sayı: ");
        x = input.nextInt();
        System.out.println("Üs olacak sayı: ");
        y = input.nextInt();
        int result = 1;

        
        for(int i = 1; i <= y; i++) {
            result *= x;
        }
        System.out.println("Cevap: " + result);
    }
}
